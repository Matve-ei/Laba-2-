#include <stdio.h> 
int main()
{
    int a, b; // целые положительные числа 
    printf("A:");
    scanf_s("%d", &a);

    printf("B:");
    scanf_s("%d", &b);
    while ((a != 0) & (b != 0)) {
        if (a > b) a = a % b; //12/8 = 8 + (4)!

        else
            if (b > a) b = b % a;
               else {
                printf("%d\n", a);
        return 0;
    }
    }
    printf("%d\n", a + b);
    return 0;
}
